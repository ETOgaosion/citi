page {
  width: 100vw;
  height: 160vh;
}

.container {
  margin-top: 2px;
  width: 100vw;
  height: 81vh;
  padding: 0em;
}
.username{
  font-size: 13px;
  text-align: center;
}
.topcontainer{
  padding-top: 0%;
  width: 100%;
  display: -webkit-flex;
  display: flex;
  justify-content: flex-start;
}
.scancode{
  margin-left: 25rpx;
  margin-top: 1%;
  width: 6%;
}
.search{
  width:80%;
  height: 66rpx;
  margin-top: 8rpx;
  justify-content: flex-start;
  position: relative;
  padding: 2px 5px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
}
.search input{
  border: 1rpx solid #e3e3e3;
  width: 100%;
  height: 66rpx;
  font-size: 13px;
  border-radius: 30rpx;
  padding-left: 32rpx;
}
.search image {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 24rpx;
  right: 32rpx;
}
.position{
  width: 8%;
  margin-top: 1%;
  margin-right: 10rpx ;
}
.notice{
  background: rgba(96, 120, 255, 0.801);
  color: rgb(255, 255, 255);  
  padding: 8rpx 16rpx;
  height: 20px;
  margin-top: 5px;
  margin-left: 5px;
  margin-bottom: 5px;
  width: 93%;
  padding: 8rpx 16rpx;
  border-radius: 32rpx;
  font-size: 13px;  
}
.notice-swiper{
  width: 100%;
  height: 20px;
}
.notice-swiper-item{
  width: 100%;
  display: flex;
  align-items: center;
}
.notice-swiper-item-text{
  margin-left: 8rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.recom-bg{
  background-color: antiquewhite;
  width: 100%;
}
.recom{
  width: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: 	#FF69B4;
  margin-left: 15rpx;
  margin-right: 15rpx;
  text-align: center;
  font-size: 13px;
  height: 30px;
}
.recom-text{
  color: white;
  font-weight: 550;
}
.swiper{
  background-color: antiquewhite;
  width: 100%;
}
.swiper-self{
  width: auto;
  margin-left: 15rpx;
  margin-right: 15rpx;
}
.content-container {
  margin-top: 5px;
  width: 100%;
  display: flex;
  flex-direction: column;
  flex-basis:auto;
}
.showboard-head{
  display: flex;
  flex-direction: row;
}
.showboard-head:first-child{
  width: 100%;
}
.showboard-head-right{
  flex:0 0 1;
}
.showborad-title1{
  display: flex;
  padding-left: 0%;
  padding-right: 0%;
  padding-top: 5px;
  padding-bottom: 5px;
  width: 100%;
  height: 40px;
  text-align: center;
  background-color: rgba(112, 60, 233, 0.322);
}
.showborad-title1-text{
  margin: 11px;
  margin-left: 10px;
  font-size: 14px;
  justify-content: center;
  align-items: center;
}
.showboard2{
  margin-top: 1px;
  display: flex;
  flex-direction: row;
  justify-content: start;
}
.showboard-scroll{
  height: 90vh;
}
.showboard-items{
  width: 145%;
  display: flex;
  height: 90vh;
}
.showboard-items .showboard-item .showboard-item-text{
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  padding-left: 63rpx;
  padding-right: 63rpx;
  text-align: center;
  display: inline-block;
  font-size: 13px;
  color: #070707;
  background-color: rgba(249, 250, 208, 0.144);
  border-bottom: rgba(49, 49, 49, 0.137);
  border-bottom-width: 1px;
  border-bottom-style: solid;
  line-height: 1.2;
  pointer-events: none;
}
.showboard-items .text-active{
  background-color: rgb(254, 255, 169);
  font-size: 13px;
  border-left: 4rpx solid #d81e06;
  color: #d81e06;
  font-weight: bold;
}
.active{
  border-left: rgb(160, 16, 100);
  background-color: rgb(255, 244, 147);
  width: 94%;
}
.text-active{
  padding-top: 35rpx;
  padding-bottom: 35rpx;
  padding-left: 60rpx;
  padding-right: 60rpx;
  text-align: center;
  display: inline-block;
  font-size: 13px;
  color: #070707;
  background-color: rgba(249, 250, 208, 0.144);
  border-bottom: rgba(49, 49, 49, 0.137);
  border-bottom-width: 1px;
  border-bottom-style: solid;
  line-height: 1.2;
  pointer-events: none;
}
.showboard-head-right{
  display: flex;
  flex-grow: 1;
  flex-direction: column;
}
.showboard-head-right:first-child{
  width: 100%;
}
.ordering{
  height: 38px;
  width: 100%;
  border-bottom-style:solid;
  border-bottom-color: gray;
  padding-top: 10px;
  padding-left: 10px;
}
.ordering-text{
  font-size: 13px;
}
.showboard2{
  display: flex;
}
@import'/components/common.wxss';
.shops{
  width: inherit;
}
.vtabs-content-item{
  border-bottom-color: turquoise;
  border-bottom-style: solid;
  height: 104vh;
}
.shops-container{
  margin-top: 10px;
  width: 61%;
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  box-sizing: content-box;
  padding: 0rpx;
  background-color: #F2f2f2;
}
.shops-box{
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  width: 262rpx;
  height: 346rpx;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 24rpx;
  border-radius:5px;
}
.shops-box .img-box{
  width: 249rpx;
  height: 249rpx;
  overflow: hidden;
}
.shops-box .img-box image{
  width: 249rpx;
  height: 249rpx;
}
.shops-box .shops-title{
  width: 119;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 13px;
  padding: 24rpx 0 0rpx 0;
  color:#000;
  margin-left: 16rpx;
}
.shops-box .shops-price{
  width: 152rpx;
  overflow: hidden;
  font-size: 11px;
  padding: 28rpx 0;
  color:#e64340;
  margin-left: 21rpx;
}
.shops-text{
  display: flex;
  flex-direction: row;
}
.shops-text:first-child{
  width: 35%;
}